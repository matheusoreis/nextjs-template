	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"typeorm": "ts-node ./node_modules/typeorm/cli",
		"sqlite:generate": "npm run typeorm -- -d ./src/infrastructure/database/configs/sqlite.ts migration:generate ./src/infrastructure/database/migrations/sqlite/%npm_config_name%",
		"sqlite:run": "npm run typeorm migration:run -- -d ./src/infrastructure/database/configs/sqlite.ts",
		"sqlite:revert": "npm run typeorm migration:revert -- -d ./src/infrastructure/database/configs/sqlite.ts",
		"postgres:generate": "npm run typeorm -- -d ./src/infrastructure/database/configs/postgres.ts migration:generate ./src/infrastructure/database/migrations/postgres/%npm_config_name%",
		"postgres:run": "npm run typeorm migration:run -- -d ./src/infrastructure/database/configs/postgres.ts",
		"postgres:revert": "npm run typeorm migration:revert -- -d ./src/infrastructure/database/configs/postgres.ts",
		"mariadb:generate": "npm run typeorm -- -d ./src/infrastructure/database/configs/mariadb.ts migration:generate ./src/infrastructure/database/migrations/mariadb/%npm_config_name%",
		"mariadb:run": "npm run typeorm migration:run -- -d ./src/infrastructure/database/configs/mariadb.ts",
		"mariadb:revert": "npm run typeorm migration:revert -- -d ./src/infrastructure/database/configs/mariadb.ts"
	},
